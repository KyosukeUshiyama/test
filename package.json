{
  "name": "test",
  "version": "0.0.0",
  "description": "Readme",
  "main": "sum.js",
  "dependencies": {
    "eslint-plugin-sonarjs": "^0.19.0"
  },
  "devDependencies": {"eslint": "^6.7.2"},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyosukeUshiyama/test.git"
  },
  "author": "kyosuke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KyosukeUshiyama/test/issues"
  },"eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "vue/max-attributes-per-line": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "vue/html-self-closing": [
        "warn",
        {
          "html": {
            "void": "always"
          }
        }
      ],
      "vue/singleline-html-element-content-newline": [
        "warn",
        {
          "ignores": [
            "v-icon"
          ]
        }
      ]
    }
  },
  "homepage": "https://github.com/KyosukeUshiyama/test#readme"
}
